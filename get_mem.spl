alias counter R4;

//Initialising counter to beginning of free pages in memory 
counter = 76;

//Searching for a free page
while(counter < 128) do
	if([MEMORY_FREE_LIST + counter] == 0) then
		break;
	endif;
	counter = counter + 1;
endwhile;

[MEMORY_FREE_LIST + counter] = 1;

//Setting result
R0 = counter;

return;
